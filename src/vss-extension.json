{
  "manifestVersion": 1,
  "id": "WIDetailsWidgetTest",
  "version": "1.15.9",
  "name": "WI Details Widget Test",
  "scopes": [ "vso.work", "vso.dashboards_manage" ],
  "description": "View details of work item on your dashboard.",
  "publisher": "DevMikaelKrief",
  "icons": {
    "default": "static/images/logo.png"
  },
  "categories": [
    "Plan and track"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Work Items",
    "Widget"
  ],
  "links": {
    "repository": {
      "uri": "https://github.com/ALM-Rangers/Work-Item-Details-Widget-Extension"
    },
    "issues": {
      "uri": "https://github.com/ALM-Rangers/Work-Item-Details-Widget-Extension/issues"
    },
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ALM-Rangers/Work-Item-Details-Widget-Extension"
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "content": {
    "license": {
      "path": "mp_terms.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "overview.md",
      "addressable": true
    },
    {
      "path": "ThirdPartyNotices.txt",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/applicationinsights-js/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/jquery/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/jquery-dotdotdot/src",
      "addressable": true,
      "packagePath": "lib"
    }
  ],
  "contributions": [
    {
      "id": "addToDashboard-work-item-menu",
      "type": "ms.vss-web.action-provider",
      "description": "",
      "targets": [
        "ms.vss-work-web.work-item-context-menu"
      ],
      "properties": {
        "group": "contributed",
        "uri": "static/addtodashboard.html"
      }
    },
    {
      "id": "widetailswidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".widetailswidget-Configuration"
      ],
      "properties": {
        "name": "Work Item Details",
        "description": "View details of work item on your dashboard.",
        "uri": "static/index.html",
        "isNameConfigurable": false,
        "catalogIconUrl:": "static/images/logo.png",
        "previewImageUrl": "static/images/logo.png",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 2
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    },
    {
      "id": "widetailswidget-Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
      "properties": {
        "name": "Work Item Details Configuration",
        "description": "Configure Work Item Details widget.",
        "uri": "static/configuration.html"
      }
    }

  ]
}
