{
  "scripts": {
    "build": "npm run copy-lib && npm run lint && npm run clean-before-build && npm run webpack && npm run copy-config",
    "postbuild": "npm run clean-after-build && npm run gallery-publish",
    "copy-lib": "cpx node_modules/telemetryclient-team-services-extension/lib/*.* scripts",
    "package": "tfx extension create  --rev-version",
    "gallery-publish": "tfx extension publish --rev-version --token 53plbqfsibqnynajzc3ac2gvayqdaz2kz65bv7a47nmjduwmbruq",
    "clean-before-build": "rimraf ./dist && rimraf ./*.vsix",
    "clean-after-build": "rimraf ./scripts/telemetryclient.js && rimraf ./scripts/telemetryclient.d.ts",
    "lint": "tslint -c tslint.json \"scripts/*.ts",
    "webpack": "webpack --progress --colors --config webpack.config.js --output-path ./dist -p",
    "copy-config": "cpx scripts/appConfig.json dist"
  },
  "devDependencies": {
    "tfx-cli": "^0.4.5",
    "cpx": "^1.5.0",
    "jasmine": "^2.5.1",
    "rimraf": "^2.5.4",
    "typescript": "2.3.4",
    "ts-loader": "^2.0.3",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "json-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "@types/jquery": "^2.0.34",
    "@types/q": "0.0.32",
    "applicationinsights-js": "*",
    "jquery": "2.2.0",
    "jquery-dotdotdot": "^1.8.2",
    "vss-web-extension-sdk": "^2.115.0",
    "telemetryclient-team-services-extension": "*"
  },
